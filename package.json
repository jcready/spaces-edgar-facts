  "main": "index.js",
  "scripts": {
    "test": "set NODE_ENV=test && standard **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://jcready@github.com/jcready/spaces-edgar-facts.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/jcready/spaces-edgar-facts/issues"
  },
  "homepage": "https://github.com/jcready/spaces-edgar-facts#readme",
  "dependencies": {
    "edgar-facts": "^1.0.8"
  },
  "lanetix": {
    "defaultParameters": {
      "command": "/edgar-fact"
    },
    "handlers": [
      {
        "provisionToken": true,
        "subject": "spacesPostCreated",
        "handler": "index",
        "contentPattern": {
          "type": "message",
          "text": {
            "Fn::GetParameter": [
              "command"
            ]
          }
        }
      }
    ]
  },
  "devDependencies": {
    "standard": "^8.6.0"
  }
}
